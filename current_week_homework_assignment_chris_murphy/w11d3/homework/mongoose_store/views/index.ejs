<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("partials/head") %>
</head>
<body>
    <h1>Products</h1>
    <div class="container">
       <% for(let i = 0; i < products.length; i++){ %>
        <div class="products">
            <h3><a href="/products/<%= products[i].id%>"><%= products[i].name %></a></h3>
            <img src=<%= products[i].img %> alt=<%= products[i].name%>>
            <h4>Price: $<%= products[i].price %></h4>
        </div>
       <% } %>
    </div>
</body>
</html>